{
  "name": "Bilkent STARS Grade Notifications",
  "version": "0.5",
  "manifest_version": 2,
  "description": "Bilkent STARS Grade Notifications",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
	  "default_title": "Bilkent STARS Grade Notifications",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs", "*://*.bilkent.edu.tr/*", "notifications"
  ]
}
